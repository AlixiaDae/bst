{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console" : "off",
        "max-classes-per-file" : "off",
        "no-plusplus" : "off",
        "class-methods-use-this" : "off",
        "no-param-reassign" : "off",
        "no-else-return" : "off",
        "no-lonely-if" : "off",
        "consistent-return" : "off"
    }
}
